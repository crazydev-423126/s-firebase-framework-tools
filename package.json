    "fuzzy": "^0.1.3",
    "inquirer": "^8.2.0",
    "inquirer-autocomplete-prompt": "^1.4.0",
    "ora": "^5.4.1",
    "triple-beam": "^1.3.0",
    "tslib": "^2.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cli-color": "^2.0.2",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^16.11.12",
    "@types/triple-beam": "^1.3.2",
